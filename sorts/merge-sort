#include <iostream>
#include <vector>

using namespace std;

void merge_sort(vector<int>& myarr, int left, int right) {
	if (left == right) {
		return;
	}

	int mid = (left + right) / 2;
	merge_sort(myarr, left, mid);
	merge_sort(myarr, mid + 1, right);

	vector<int> newarr;
	for (int i = left, j = mid + 1; i <= mid || j <= right;) {
		if (i > mid) {
			newarr.push_back(myarr[j++]);
		}
		else if (j > right) {
			newarr.push_back(myarr[i++]);
		}
		else if (myarr[i] <= myarr[j]) {
			newarr.push_back(myarr[i++]);
		}
		else {
			newarr.push_back(myarr[j++]);
		}
	}
	for (int i = 0; i < newarr.size(); i++) {
		myarr[left + i] = newarr[i];
	}
}

int main() {
	int n;
	cin >> n;
	vector <int> myarr(n);
	for (int i = 0; i < n; i++) {
		cin >> myarr[i];
	}
	merge_sort(myarr, 0, n - 1);
	for (int i = 0; i < n; i++) {
		cout << myarr[i] << ' ';
	}
}
