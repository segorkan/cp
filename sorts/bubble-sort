#include <iostream>
#include <vector>
#include <algorithm>


using namespace std;



int main() {
	long long n, count = 0;
	cin >> n;
	vector <long long> myarr;
	for (int i = 0; i < n; i++) {
		int h;
		cin >> h;
		myarr.push_back(h);
	}
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n - i - 1; j++) {
			if (myarr[j] > myarr[j + 1]) {
				swap(myarr[j], myarr[j + 1]);
				count++;
			}
		}
	}
	cout << count;
}
