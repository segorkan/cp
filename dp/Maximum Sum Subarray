#include <iostream>
#include <vector>
#include <algorithm>


using namespace std;


int main()
{
	const long long MIN = -1e6;
	int n, ans = MIN;
	cin >> n;
	vector <int> myarr(n), dp(n);
	for (int i = 0; i < n; i++) cin >> myarr[i];
	dp[0] = myarr[0];
	for (int i = 1; i < n; i++) {
		dp[i] = max(dp[i - 1] + myarr[i], myarr[i]);
		ans = max(ans, dp[i]);
	}
	cout << ans;
}
