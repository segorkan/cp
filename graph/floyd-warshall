#include <iostream>
#include <algorithm>
#include <vector>
using namespace std;
 
int main()
{
    int n, maxi = 0;
    cin >> n;
    const long long INF = 1000000;
    vector <vector<int>> graph(n + 1, vector<int>(n + 1));
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= n; j++) {
            cin >> graph[i][j];
            if (graph[i][j] == -1) graph[i][j] = INF;
        }
    }
    for (int k = 1; k <= n; k++) {
        for (int i = 1; i <= n; i++) {
            for (int j = 1; j <= n; j++) {
                graph[i][j] = min(graph[i][j], graph[i][k] + graph[k][j]);
            }
        }
    }
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= n; j++) {
            if (graph[i][j] == INF) continue;
            if (graph[i][j] > maxi) maxi = graph[i][j];
        }
    }
    cout << maxi;
}
